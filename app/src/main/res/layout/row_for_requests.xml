<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/NovigradYellow">

    <RelativeLayout
        android:layout_width="405dp"
        android:layout_height="127dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView28"
            android:layout_width="wrap_content"
            android:layout_height="28dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="17dp"
            android:layout_marginTop="7dp"
            android:text="Service requested :"
            android:textColor="#302C2C" />

        <TextView
            android:id="@+id/textViewRequestSender"
            android:layout_width="157dp"
            android:layout_height="wrap_content"
            android:layout_above="@+id/textViewStatus"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="47dp"
            android:layout_marginEnd="28dp"
            android:layout_marginBottom="17dp"
            android:layout_toEndOf="@+id/textViewRequestSender2"
            android:text="Request Provenance"
            android:textAlignment="viewEnd"
            android:textColor="#121010" />

        <TextView
            android:id="@+id/textViewRequestSender2"
            android:layout_width="154dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView28"
            android:layout_alignParentStart="true"
            android:layout_marginStart="17dp"
            android:layout_marginTop="26dp"
            android:text="Request Provenance :"
            android:textAlignment="textStart"
            android:textColor="#514D4D" />

        <TextView
            android:id="@+id/textViewTypeOfRequest"
            android:layout_width="214dp"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="43dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="28dp"
            android:layout_toEndOf="@+id/textView28"
            android:text="TextView"
            android:textAlignment="textEnd"
            android:textColor="#000000"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textViewStatus"
            android:layout_width="156dp"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="47dp"
            android:layout_marginEnd="28dp"
            android:layout_marginBottom="15dp"
            android:layout_toEndOf="@+id/textViewStatus2"
            android:text="Status"
            android:textAlignment="viewEnd"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/textViewStatus2"
            android:layout_width="156dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textViewRequestSender2"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="18dp"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="15dp"
            android:text="Status:"
            android:textAlignment="textStart"
            android:textColor="#3C3B3B" />
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>