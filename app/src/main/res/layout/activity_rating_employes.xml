<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg1"
    tools:context=".RatingEmployes">

    <ImageView
        android:id="@+id/imageLogo"
        android:layout_width="200dp"
        android:layout_height="70dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="112dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="99dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <ImageButton
        android:id="@+id/btnStark1"
        android:layout_width="53dp"
        android:layout_height="49dp"
        android:layout_below="@+id/textopinion"
        android:layout_alignParentStart="true"
        android:layout_marginStart="29dp"
        android:layout_marginTop="49dp"
        android:background="#FFFFFF"
        android:contextClickable="true"
        app:srcCompat="@android:drawable/btn_star" />

    <ImageButton
        android:id="@+id/btnStark2"
        android:layout_width="53dp"
        android:layout_height="49dp"
        android:layout_alignTop="@+id/btnStark1"
        android:layout_marginStart="23dp"
        android:layout_marginTop="0dp"
        android:layout_toEndOf="@+id/btnStark1"
        android:background="#FFFFFF"
        app:srcCompat="@android:drawable/btn_star" />

    <ImageButton
        android:id="@+id/btnStark3"
        android:layout_width="53dp"
        android:layout_height="49dp"
        android:layout_alignTop="@+id/btnStark2"
        android:layout_marginStart="15dp"
        android:layout_marginTop="-1dp"
        android:layout_toEndOf="@+id/btnStark2"
        android:background="#FFFFFF"
        app:srcCompat="@android:drawable/btn_star" />

    <ImageButton
        android:id="@+id/btnStark4"
        android:layout_width="53dp"
        android:layout_height="49dp"
        android:layout_alignTop="@+id/btnStark3"
        android:layout_marginStart="18dp"
        android:layout_marginTop="0dp"
        android:layout_toEndOf="@+id/btnStark3"
        android:background="#FFFFFF"
        app:srcCompat="@android:drawable/btn_star" />

    <ImageButton
        android:id="@+id/btnStark5"
        android:layout_width="53dp"
        android:layout_height="49dp"
        android:layout_alignTop="@+id/btnStark4"
        android:layout_marginStart="23dp"
        android:layout_marginTop="-2dp"
        android:layout_toEndOf="@+id/btnStark4"
        android:background="#FFFFFF"
        app:srcCompat="@android:drawable/btn_star" />

    <Button
        android:id="@+id/finishRating"
        android:layout_width="134dp"
        android:layout_height="59dp"
        android:layout_below="@+id/btnStark5"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="132dp"
        android:layout_marginEnd="8dp"
        android:text="Finsh" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="wrap_content"
        android:layout_height="208dp"
        android:layout_below="@+id/imageLogo"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="14dp"
        app:srcCompat="@drawable/bg_login_v2" />

    <TextView
        android:id="@+id/textopinion"
        android:layout_width="296dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="18dp"
        android:text="What do you think about the quality of our service ?"
        android:textAppearance="@style/TextAppearance.AppCompat.Large" />

    <TextView
        android:id="@+id/text"
        android:layout_width="204dp"
        android:layout_height="47dp"
        android:layout_alignTop="@+id/imageView15"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="46dp"
        android:layout_marginEnd="104dp"
        android:text="Your opinion count"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="@color/NovigradBlue"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="86dp"
        android:layout_height="32dp"
        android:layout_below="@+id/finishRating"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="69dp"
        android:layout_marginEnd="163dp"
        app:srcCompat="@drawable/glbmlogo" />

</RelativeLayout>